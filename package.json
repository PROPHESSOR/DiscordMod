{
  "name": "dmpatchcreator",
  "version": "0.0.1",
  "description": "It's a DiscordMod patch creator!",
  "scripts": {
    "create": "diff -ru1 origin asar > patch.diff; md5sum /usr/share/discord/resources/_app.asar | awk '{ print $1 }' > md5.txt; exit 0",
    "init": "asar e /usr/share/discord/resources/_app.asar origin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PROPHESSOR/DiscordModAPI.git"
  },
  "keywords": [
    "discord",
    "mod",
    "injector",
    "discordmod"
  ],
  "author": "PROPHESSOR",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PROPHESSOR/DiscordModAPI/issues"
  },
  "homepage": "https://github.com/PROPHESSOR/DiscordModAPI#readme",
  "dependencies": {
    "asar": "latest",
    "diff": "^3.4.0",
    "md5": "^2.2.1"
  }
}
